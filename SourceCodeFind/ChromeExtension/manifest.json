{
  "manifest_version": 2,

  "name": "Source Code Copier",
  "description": "Source Code Copier Plugin!",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "38": "icon38.png",
    "19": "icon19.png"
  },

  "browser_action": {
    "default_icon": "icon128.png",
	"default_popup": "popup.html"
  },
  "permissions": [
     "activeTab",
	 "tabs",
	 "http://localhost:3000/insert"
  ]
}